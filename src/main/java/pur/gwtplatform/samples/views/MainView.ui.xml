<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default' xmlns:p1="urn:import:com.google.gwt.user.cellview.client">

	<g:HTMLPanel>
		<g:LayoutPanel width="998px" height="848px">
			<g:layer left="101px" width="437px" top="22px" height="433px">
				<g:VerticalPanel ui:field="panel1" borderWidth="2">
					<g:Label text="Données stoquées" ui:field="labelStock" styleName="gwt-RichTextToolbar"/>
					<p1:DataGrid width="400px" height="300px" ui:field="dataGrid" title="Les données stoquées dans le Local Storage"/>
				</g:VerticalPanel>
			</g:layer>
			<g:layer left="529px" width="343px" top="25px" height="342px">
				<g:VerticalPanel spacing="10">
					<g:Button ui:field="validerButton" text="Insérer storage" width="140px">Valider</g:Button>
					<g:Button text="Ouvrir popup de saisie" ui:field="opsButton"/>
					<g:Button text="Ouvrir popup de suppression" ui:field="opdButton"/>
					<g:Button ui:field="asrButton" width="140px">Appel Service REST</g:Button>
					<g:HorizontalPanel width="323px" verticalAlignment="ALIGN_MIDDLE">
						<g:Label text="Clé à supprimer:" horizontalAlignment="ALIGN_RIGHT" styleName="gwt-RichTextToolbar"/>
						<g:SuggestBox ui:field="sBox"/>
					</g:HorizontalPanel>
				</g:VerticalPanel>
			</g:layer>
		</g:LayoutPanel>
		
	</g:HTMLPanel>
</ui:UiBinder>
